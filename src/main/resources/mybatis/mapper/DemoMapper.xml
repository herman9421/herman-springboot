<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.herman.springboot.demo.mapper.DemoMapper">

    <select id="get" parameterType="java.lang.String" resultType="com.herman.springboot.demo.entity.Demo">
        SELECT
            id id,
            is_deleted isDeleted,
            gmt_create gmtCreate,
            gmt_modified gmtModified
        FROM
            demo
        WHERE id=#{id}
    </select>

</mapper>
